{
  "name": "agent-dashboard",
  "version": "4.9.0-rc1",
  "main": "index.js",
  "scripts": {
    "test": "node scripts/check-open-core-boundaries.mjs && pnpm -s test:smoke && pnpm -s test:unit && pnpm -s test:regression",
    "test:boundaries": "node scripts/check-open-core-boundaries.mjs",
    "test:smoke": "WS_SMOKE=1 node scripts/ws_smoke_test.mjs",
    "test:unit": "node --test test/artifactManager.validation.test.mjs test/autoVerify.test.mjs test/autoVerifyFailure.test.mjs test/budget.test.mjs test/plan.test.mjs test/report-api.test.mjs test/report.test.mjs test/safety.test.mjs test/stallWatchdog.test.mjs test/complianceRunner.test.mjs test/platform-compliance.test.mjs",
    "test:unit:quarantine": "node --test test/*.quarantine.mjs",
    "test:integration:sprint5": "node --test test/sprint5-features.quarantine.mjs",
    "test:regression": "node scripts/plan_merge_regression.mjs && node scripts/danger_gating_regression.mjs",
    "test:regression:plan_merge": "node scripts/plan_merge_regression.mjs",
    "test:regression:danger_gating": "node scripts/danger_gating_regression.mjs",
    "test:production": "bash scripts/test-production-sim.sh",
    "pr:update:rc1": "bash scripts/update_pr_rc1.sh",
    "staging:sanity": "bash scripts/staging_sanity.sh",
    "build": "vite build --config ui/vite.config.js",
    "build:ui": "vite build --config ui/vite.config.js",
    "predev": "pnpm -s build:ui",
    "start": "node server.mjs",
    "dev": "node server.mjs",
    "artifact:health": "node scripts/artifact-health-check.mjs",
    "artifact:monitor": "node scripts/artifact-monitor.mjs",
    "artifact:report": "node scripts/artifact-report.mjs",
    "deploy:validate": "bash scripts/deploy-production.sh",
    "deploy:setup-monitoring": "bash scripts/setup-monitoring.sh",
    "audit:open": "node scripts/audit_open_release.mjs",
    "pack:open": "node scripts/pack_open_release.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "minimatch": "^10.0.1",
    "node-pty": "^1.1.0",
    "openai": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ws": "^8.19.0",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wscat": "^6.1.0"
  },
  "type": "module"
}
